{
  "name": "smart-contract-management",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "set-module": "node set-module.js",
    "set-commonjs": "node set-commonJs.js",
    "dev": "npm run set-module && vite",
    "compile": "npm run set-commonjs && truffle compile && npm run set-module",
    "migrate": "npm run set-commonjs && truffle migrate --network sepolia && npm run set-module",
    "start": "npm run set-module && react-scripts start",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2",
    "@truffle/hdwallet-provider": "^2.1.15",
    "dotenv": "^16.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loader-spinner": "^6.1.6",
    "solc": "^0.8.26"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "ethers": "^5.7.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "truffle": "^5.11.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.0"
  },
  "type": "module"
}